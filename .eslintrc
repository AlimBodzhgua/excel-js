{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
	},
	"rules": {
		"semi": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"linebreak-style": "off",
		"eol-last": ["error", "never"],
		"indent": 0,
		"no-tabs": 0,
		'no-trailing-spaces': 0,
		'operator-linebreak': 'off',
		'spaced-comment': 'off',
	},
	"extends": ["eslint:recommended", "google"],
}